{
    "name": "leoknudsen/custom_bootstrap_theme",
    "description": "Custom frontend theme inheriting bootstrap package",
    "type": "library",
    "require": {
        "typo3/cms-core": "*",
        "bk2k/bootstrap-package": "*"
    },
    "extra": {
    	"typo3/cms": {
		    "extension-key": "custombootstraptheme"
	    }
    },
    "replace": {
        "custombootstraptheme": "self.version",
        "typo3-ter/custombootstraptheme": "self.version"
    }
}
